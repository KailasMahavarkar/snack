    "eslint-plugin-react-hooks": "^4.6.0",
    "node-polyfill-webpack-plugin": "^2.0.0",
    "patch-package": "^6.4.7",
    "snack-babel-standalone": "npm:@bycedric/snack-babel-standalone-test",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0"
