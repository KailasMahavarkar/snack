{
  "extends": "@tsconfig/node18",
  "compilerOptions": {
    "allowJs": true,
    "noEmit": true,
    "noImplicitAny": false,
    "isolatedModules": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "useUnknownInCatchVariables": false,
    "baseUrl": "./"
  },
  "include": ["./src"]
}
