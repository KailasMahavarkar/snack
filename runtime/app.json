{
  "name": "Snack",
  "expo": {
    "name": "Snack",
    "description": "Write code in Expo's online editor and instantly use it on your phone",
    "scheme": "snack",
    "owner": "exponent",
    "slug": "snack",
    "version": "1.0.0",
    "primaryColor": "#000",
    "icon": "https://exp-brand-assets.s3.amazonaws.com/SnackIcon_200.png",
    "notification": {
      "icon": "https://exp-us-standard.s3.amazonaws.com/placeholder-push-icon-blue-circle.png",
      "color": "#000000"
    },
    "splash": {
      "image": "./assets/splash.png"
    },
    "developmentClient": {
      "silentLaunch": true
    },
    "platforms": ["android", "ios", "web"],
    "assetBundlePatterns": ["**/*"],
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    }
  }
}
